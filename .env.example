################################################################################
# FRONTEND - EXPO/REACT NATIVE ENVIRONMENT
################################################################################
# Copy this file to .env.local and fill in your actual values
# Note: Expo requires EXPO_PUBLIC_ prefix for client-side variables

# Supabase Configuration (Client-Side)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Backend API URL
# Default for local development: http://localhost:8000
# For Docker production: http://backend:8000
EXPO_PUBLIC_BACKEND_API_URL=http://localhost:8000

################################################################################
# APPLICATION FEATURES
################################################################################

# Location Services (optional)
# EXPO_PUBLIC_ENABLE_LOCATION=true
